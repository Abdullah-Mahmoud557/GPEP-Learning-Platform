@model PersonalizationProfiles

<div class="container">
    <h2>Personalization Profile Details</h2>
    <div class="card p-4">
        <p><strong>Learner ID:</strong> @Model.LearnerID</p>
        <p><strong>Profile ID:</strong> @Model.ProfileID</p>
        <p><strong>Preferred Content Type:</strong> @Model.PreferredContentType</p>
        <p><strong>Emotional State:</strong> @Model.EmotionalState</p>
        <p><strong>Personality Type:</strong> @Model.PersonalityType</p>
    </div>
    <a asp-action="Index" class="btn btn-secondary mt-3">Back to List</a>
</div>
